{
  "name": "micro-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "yarn && npm-run-all --serial install:*",
    "start": "yarn start:main-vue2",
    "start:main-vue2": "npm-run-all --parallel build:watch start-child:* main-vue2",
    "build:watch": "cross-env NODE_ENV='development' rollup -c -w",
    "build": "cross-env NODE_ENV='production' rollup -c && npm run createtype && npm run afterbuild",
    "install:main-vue2": "cd examples/main-vue2 && yarn",
    "install:child-react16": "cd examples/children/vue2 && yarn",
    "main-vue2": "cd examples/main-vue2 && yarn serve",
    "start-child:react16": "cd examples/children/vue2 && yarn serve"
  },
  "dependencies": {
    "@micro-zoe/micro-app": "^0.4.3",
